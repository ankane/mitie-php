{
    "name": "ankane/mitie",
    "description": "Named-entity recognition for PHP",
    "license": "BSL-1.0",
    "support": {
        "issues": "https://github.com/ankane/mitie-php/issues",
        "source": "https://github.com/ankane/mitie-php"
    },
    "authors": [
        {
            "name": "Andrew Kane",
            "email": "andrew@ankane.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "Mitie\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">= 8.2",
        "ext-ffi": ">= 8.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^11"
    },
    "scripts": {
        "test": "phpunit tests",
        "valgrind": "valgrind --leak-check=full --trace-children=yes --error-exitcode=1 --quiet phpunit tests",
        "post-install-cmd": "Mitie\\Vendor::check",
        "post-update-cmd": "Mitie\\Vendor::check"
    }
}
